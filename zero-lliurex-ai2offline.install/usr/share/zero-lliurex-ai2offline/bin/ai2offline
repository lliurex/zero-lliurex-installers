#!/bin/bash
RELEASE=7.7
# we might need to kill old processes

for PROC in $(pgrep -f appengine-tools-api.jar); do 
	kill -9 $PROC;
done;

for PROC in $(pgrep -f BuildServer.jar); do
        kill -9 $PROC;
done;


cd /opt/ai2offline/AI2Offline${RELEASE}/

# ai2 server
/opt/ai2offline/AI2Offline${RELEASE}/startAIServer.sh &

#Takes some time for server to be online. hopefully this is enough
sleep 5

xdg-open http://localhost:8888 &

# ai2 build server
/opt/ai2offline/AI2Offline${RELEASE}/startBuildServer.sh &

# ai2 starter
/opt/ai2offline/AI2Offline${RELEASE}/startAIStarter.sh 




